<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mju.ict.repository.ICategoryDAO">

	<select id="selectAllCategories" resultType="com.mju.ict.model.Category">
		SELECT * FROM tb_category
	</select>
	
		<select id="selectCategoriesByLarge" parameterType="String" resultType="com.mju.ict.model.Category">
		SELECT * FROM tb_category
		where category_code2 = #{large}
	</select>

</mapper>