<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mju.ict.repository.IQuestionCategoryDAO">
	<select id="selectAllQuestionCategories" resultType="com.mju.ict.model.QuestionCategory">
		SELECT * FROM tb_question_category
	</select>
	
	<select id="selectProductQuestionCategory" resultType="com.mju.ict.model.QuestionCategory">
		SELECT * FROM tb_question_category
		WHERE
		question_category_title = "상품정보문의"
	</select>
</mapper>